<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.cms.ticket.services.persistence.dao.TicketStrategyMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.jfeat.cms.ticket.services.persistence.model.TicketStrategy">
		<id column="id" property="id" />
		<result column="name" property="name" />
		<result column="execute_department_id" property="executeDepartmentId" />
		<result column="execute_department_name" property="executeDepartmentName" />
		<result column="type_id" property="typeId" />
		<result column="type_name" property="typeName" />
		<result column="practices" property="practices" />
		<result column="practices_step" property="practicesStep" />
		<result column="content" property="content" />
		<result column="is_cyclcity" property="isCyclcity" />

	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, number, name,is_cyclcity as isCyclcity, execute_department_id AS executeDepartmentId, execute_department_name AS executeDepartmentName, type_id AS typeId, type_name AS typeName, practices, practices_step AS practicesStep, content
    </sql>

</mapper>
