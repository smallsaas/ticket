<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.evaluation.services.persistence.dao.StockEvaluationMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.jfeat.am.module.evaluation.services.persistence.model.StockEvaluation">
		<id column="id" property="id" />
		<result column="stock_id" property="stockId" />
		<result column="member_id" property="memberId" />
		<result column="member_name" property="memberName" />
		<result column="stock_type" property="stockType" />
		<result column="stock_tag" property="stockTag" />
		<result column="content" property="content" />
		<result column="is_display" property="isDisplay" />
		<result column="is_stick" property="isStick" />
		<result column="status" property="status" />
		<result column="create_time" property="createTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, stock_id AS stockId, member_id AS memberId, stock_tag as stockTag, member_name as memberName,stock_type as stockType, content, is_display AS isDisplay, is_stick AS isStick, status, create_time AS createTime
    </sql>

</mapper>
